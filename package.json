{
  "name": "@kylegillen/fastify-uws",
  "version": "1.0.0",
  "description": "uWebSockets.js for fastify",
  "keywords": [
    "fastify",
    "uWebSockets.js",
    "fastify-plugin"
  ],
  "homepage": "https://github.com/kylegillen/fastify-uws#readme",
  "bugs": {
    "url": "https://github.com/kylegillen/fastify-uws/issues"
  },
  "author": "Kyle Gillen",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kylegillen/fastify-uws.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "dev": "vitest",
    "build": "tsup",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run lint && npm run test",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack .",
    "lint": "tsc",
    "test": "vitest run"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  }
}
