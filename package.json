{
  "name": "@kylegillen/fastify-uws",
  "type": "module",
  "version": "1.0.0",
  "description": "uWebSockets.js for fastify",
  "author": "Kyle Gillen",
  "license": "MIT",
  "homepage": "https://github.com/kylegillen/fastify-uws#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kylegillen/fastify-uws.git"
  },
  "bugs": {
    "url": "https://github.com/kylegillen/fastify-uws/issues"
  },
  "keywords": [
    "fastify",
    "uWebSockets.js",
    "fastify-plugin"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run ci",
    "dev": "vitest --pass-with-no-tests",
    "build": "tsup",
    "ci": "npm run build && npm run lint && npm run check-exports && npm run check-types && npm run test",
    "lint": "eslint",
    "format": "eslint --fix",
    "check-exports": "attw --pack .",
    "check-types": "tsc",
    "test": "vitest run --pass-with-no-tests",
    "local-release": "changeset version && changeset publish"
  },
  "dependencies": {
    "@fastify/error": "^4.1.0",
    "eventemitter3": "^5.0.1",
    "fastify-plugin": "^5.0.1",
    "ipaddr.js": "^2.2.0",
    "it-pushable": "^3.2.3",
    "it-to-stream": "^1.0.0",
    "streamx": "^2.22.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.51.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.10.1",
    "@arethetypeswrong/cli": "^0.17.4",
    "@changesets/cli": "^2.28.1",
    "@types/streamx": "^2.9.5",
    "eslint": "^9.22.0",
    "fastify": "^5.2.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  }
}